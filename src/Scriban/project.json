{
  "title": "Scriban",
  "version": "0.3.0",
  "authors": [ "Alexandre Mutel" ],
  "description": "A fast and powerful text templating language and engine for .NET",
  "copyright": "Alexandre Mutel",
  "language": "en-US",
  "packOptions": {
    "owners": [ "Alexandre Mutel" ],
    "licenseUrl": "https://github.com/lunet-io/scriban/blob/master/license.txt",
    "projectUrl": "https://github.com/lunet-io/scriban",
    "iconUrl": "https://raw.githubusercontent.com/lunet-io/scriban/master/scriban.png",
    "releaseNotes": "> 0.3.0\n - Add support date.format (global date default format)\n - Add support for verbatim string escaped by backsticks `...`\n - Add support for regex functions (regex.split, regex.replace, regex.match)\n",
    "requireLicenseAcceptance": false,
    "tags": [ "templating handlebars liquid" ]
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "xmlDoc": true,
        "nowarn": [ "CS1591" ]
      }
    }
  },
  "frameworks": {
    "net35": {
      "buildOptions": {
        "define": [ "NETPRE45" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": ""
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "buildOptions": {
        "define": [ "NETPRE45" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": ""
      }
    },
    "netstandard1.1": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
       "NETStandard.Library": "1.6.0"
       }
    }
  }
}