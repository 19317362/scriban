{
  "title": "Scriban",
  "version": "1.0.0",
  "authors": [ "Alexandre Mutel" ],
  "owners": [ "Alexandre Mutel" ],
  "licenseUrl": "https://github.com/lunet-io/scriban/blob/master/license.txt",
  "projectUrl": "https://github.com/lunet-io/scriban",
  "iconUrl": "https://raw.githubusercontent.com/lunet-io/scriban/master/scriban.png",
  "requireLicenseAcceptance": false,
  "description": "A text templating language and engine for .NET",
  "copyright": "Alexandre Mutel",
  "language": "en-US",
  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "net20": {
      "compilationOptions": {
        "define": [ "NETPRE45" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": ""
      }
    },
    "net35": {
      "compilationOptions": {
        "define": [ "NETPRE45" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": ""
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "compilationOptions": {
        "define": [ "NETPRE45" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": ""
      }
    },
    "dotnet5.4": {
      "compilationOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1-beta-23516",
        "System.Runtime.Extensions": "4.0.10",
        "System.Reflection": "4.0.0",
        "System.Collections": "4.0.10",
        "System.Linq": "4.0.0",
        "System.Text.RegularExpressions": "4.0.0",
        "System.Threading": "4.0.0",
        "System.Runtime": "4.0.20",
        "Microsoft.CSharp": "4.0.0"
      }
    }
  }
}